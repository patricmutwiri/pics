<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset addfieldpath="/administrator/components/com_categories/models/fields">
		<field
			name="asset_id"
			type="hidden"
			filter="unset"
		/>
		<field
			name="id"
			type="text"
			label="COM_PERFECTPICS_FORMATS_PERFECTPICS_FORMAT_FIELD_ID_LABEL"
			description="COM_PERFECTPICS_FORMATS_PERFECTPICS_FORMAT_FIELD_ID_DESC"
			maxlength="10"
			filter="INTEGER"
			class="readonly"
			readonly="true"
		/>
		<field
			name="formats_icon"
			label="COM_PERFECTPICS_FORMATS_PERFECTPICS_FORMAT_FIELD_FORMATS_ICON_LABEL"
			description="COM_PERFECTPICS_FORMATS_PERFECTPICS_FORMAT_FIELD_FORMATS_ICON_DESC"
			type="media"
			maxlength="255"
			class="input-large input-large-text"
		>
		</field>
		<field
			name="format_title"
			label="COM_PERFECTPICS_FORMATS_PERFECTPICS_FORMAT_FIELD_FORMAT_TITLE_LABEL"
			description="COM_PERFECTPICS_FORMATS_PERFECTPICS_FORMAT_FIELD_FORMAT_TITLE_DESC"
			type="text"
			maxlength="255"
			class="input-xxlarge input-large-text"
		>
		</field>
		<field
			name="format_size"
			label="COM_PERFECTPICS_FORMATS_PERFECTPICS_FORMAT_FIELD_FORMAT_SIZE_LABEL"
			description="COM_PERFECTPICS_FORMATS_PERFECTPICS_FORMAT_FIELD_FORMAT_SIZE_DESC"
			type="text"
			maxlength="255"
		>
		</field>
		<field
			name="price_from"
			label="COM_PERFECTPICS_FORMATS_PERFECTPICS_FORMAT_FIELD_PRICE_FROM_LABEL"
			description="COM_PERFECTPICS_FORMATS_PERFECTPICS_FORMAT_FIELD_PRICE_FROM_DESC"
			type="number"
			maxlength="255"
			validate="number"
		>
		</field>
		<field
			name="pages"
			label="COM_PERFECTPICS_FORMATS_PERFECTPICS_FORMAT_FIELD_PAGES_LABEL"
			description="COM_PERFECTPICS_FORMATS_PERFECTPICS_FORMAT_FIELD_PAGES_DESC"
			type="number"
			maxlength="255"
			validate="number"
		>
		</field>
		<field
			name="format_price"
			label="COM_PERFECTPICS_FORMATS_PERFECTPICS_FORMAT_FIELD_FORMAT_PRICE_LABEL"
			description="COM_PERFECTPICS_FORMATS_PERFECTPICS_FORMAT_FIELD_FORMAT_PRICE_DESC"
			type="number"
			maxlength="100"
			validate="number"
			filter="INTEGER"
		>
		</field>
		<field
			name="catid"
			type="categoryedit"
			extension="com_perfectpics_formats"
			label="JCATEGORY"
			description="JFIELD_CATEGORY_DESC"
			class="inputbox"
			required="true"
		/>
		<field
			name="published"
			type="list"
			label="JSTATUS"
			description="JFIELD_PUBLISHED_DESC"
			class="chzn-color-state"
			size="1"
			default="1"
			filter="interval"
		>
			<option value="1">JPUBLISHED</option>
			<option value="0">JUNPUBLISHED</option>
			<option value="2">JARCHIVED</option>
			<option value="-2">JTRASHED</option>
		</field>
		<field
			name="checked_out"
			type="hidden"
			label="COM_PERFECTPICS_FORMATS_PERFECTPICS_FORMAT_FIELD_CHECKED_OUT_LABEL"
			description="COM_PERFECTPICS_FORMATS_PERFECTPICS_FORMAT_FIELD_CHECKED_OUT_DESC"
			filter="UNSET"
		/>
		<field
			name="created"
			type="calendar"
			label="JGLOBAL_FIELD_CREATED_LABEL"
			description="JGLOBAL_FIELD_CREATED_DESC"
			class="readonly"
		/>
		<field
			name="created_by"
			type="user"
			label="JGLOBAL_FIELD_CREATED_BY_LABEL"
			description="JGLOBAL_FIELD_CREATED_BY_DESC"
		/>
		<field
			name="modified"
			type="calendar"
			label="JGLOBAL_FIELD_MODIFIED_LABEL"
			description="JGLOBAL_FIELD_MODIFIED_LABEL"
			class="readonly"
		/>
		<field
			name="modified_by"
			type="user"
			label="JGLOBAL_FIELD_MODIFIED_BY_LABEL"
			description="JGLOBAL_FIELD_MODIFIED_BY_DESC"
		/>
		<field
			name="publish_up"
			type="calendar"
			label="JGLOBAL_FIELD_PUBLISH_UP_LABEL"
			description="JGLOBAL_FIELD_PUBLISH_UP_DESC"
			class="inputbox" format="%Y-%m-%d %H:%M:%S"
			labelclass="control-label"
			filter="user_utc"
		/>
		<field
			name="publish_down"
			type="calendar"
			label="JGLOBAL_FIELD_PUBLISH_DOWN_LABEL"
			description="JGLOBAL_FIELD_PUBLISH_DOWN_DESC"
			class="inputbox"
			format="%Y-%m-%d %H:%M:%S"
			labelclass="control-label"
			filter="user_utc"
		/>
		<field
			name="version"
			type="text"
			label="JGLOBAL_MODIFIED"
			description="JGLOBAL_MODIFIED"
			readonly="true"
			filter="unset"
			class="readonly"
		/>
		<field
			name="hits"
			type="text"
			label="JGLOBAL_HITS"
			description="JGLOBAL_LIST_HITS_LABEL"
			readonly="true"
			filter="unset"
			class="readonly"
		/>
		<field
			name="access"
			type="accesslevel"
			label="JFIELD_ACCESS_LABEL"
			description="JFIELD_ACCESS_DESC"
			default="1"
			class="inputbox"
			size="1"
		/>
		<field
			name="rules"
			type="rules"
			label="JFIELD_RULES_LABEL"
			translate_label="false"
			class="inputbox"
			filter="rules"
			component="com_perfectpics_formats"
			section="perfectpics_formats"
			validate="rules"
		/>
	</fieldset>
	<fields name="images">
		<fieldset name="images">
		</fieldset>
	</fields>
</form>
